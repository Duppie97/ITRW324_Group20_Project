<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaco.jamroomfinal">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ViewProfilesActivities"
            android:label="@string/title_activity_view_profiles_activities"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditProfilesActivity"
            android:label="@string/title_activity_edit_profiles"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>